<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageId>Rebirth Studio</PackageId>
    <Title>Rebirth Studio</Title>
    <Authors>Rebirth Studio</Authors>
    <Company>Rebirth Studio</Company>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Product>小黑子病毒</Product>
    <ApplicationIcon>icon~1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon~1.ico" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>